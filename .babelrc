{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-react-jsx",
    "@babel/plugin-proposal-optional-chaining",
    [
      "babel-plugin-styled-components",
      {
        "ssr": false,
        "displayName": true,
        "fileName": false
      }
    ]
  ]
}
